[run]
source = .
omit =
    */venv/*
    */migrations/*
    */tests/*
    */test_*.py
    manage.py
    */settings.py
    */__init__.py
    */celery.py
    */wsgi.py
    */asgi.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
